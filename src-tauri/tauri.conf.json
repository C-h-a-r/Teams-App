{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "https://teams.microsoft.com/_?culture=en-gb&country=gb#/school/teams-grid/General?ctx=teamsGrid",
    "distDir": "https://teams.microsoft.com/_?culture=en-gb&country=gb#/school/teams-grid/General?ctx=teamsGrid",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Teams App",
    "version": "2.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "chars.microsoft.teams.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Microsoft Teams",
        "width": 800,
        "height": 600
      }
    ]
  }
}
